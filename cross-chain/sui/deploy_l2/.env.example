# Network Selection (testnet or mainnet)
NETWORK=testnet

# Ethereum Testnet Configuration
ETHEREUM_RPC_URL_TESTNET=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
ETHEREUM_PRIVATE_KEY_TESTNET=your_ethereum_testnet_private_key_here

# Ethereum Mainnet Configuration
ETHEREUM_RPC_URL_MAINNET=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
ETHEREUM_PRIVATE_KEY_MAINNET=your_ethereum_mainnet_private_key_here

# SUI Testnet Configuration
SUI_RPC_URL_TESTNET=https://fullnode.testnet.sui.io
SUI_PRIVATE_KEY_TESTNET=your_sui_testnet_private_key_here

# SUI Mainnet Configuration
SUI_RPC_URL_MAINNET=https://fullnode.mainnet.sui.io
SUI_PRIVATE_KEY_MAINNET=your_sui_mainnet_private_key_here

# Bridge Configuration Testnet
BRIDGE_RECIPIENT_TESTNET=your_sui_testnet_address_here
BRIDGE_AMOUNT_TESTNET=0.001

# Bridge Configuration Mainnet
BRIDGE_RECIPIENT_MAINNET=your_sui_mainnet_address_here
BRIDGE_AMOUNT_MAINNET=0.001

# Withdrawal Configuration Testnet
SUI_WITHDRAWAL_PRIVATE_KEY_TESTNET=your_withdrawal_testnet_private_key_here

# Withdrawal Configuration Mainnet
SUI_WITHDRAWAL_PRIVATE_KEY_MAINNET=your_withdrawal_mainnet_private_key_here

# Wormhole Scan API
WORMHOLESCAN_API_TESTNET=https://api.testnet.wormholescan.io
WORMHOLESCAN_API_MAINNET=https://api.wormholescan.io